Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle: hintslight

! set ttymouse=xterm2

! URxvt
! General settings
URxvt.saveLines: 2000
URxvt.scrollBar: False
URxvt.scrollstyle: rxvt
URxvt.termName: xterm-256color
URxvt.urgentOnBell: True
URxvt.cursorBlink: False
URxvt.font: xft:DejaVu Sans Mono:size=10,xft:PowerlineSymbols:pixelsize=12,xft:Noto Sans Kannada:style=Regular:pixelsize=14,xft:Mona:style=Regular:pixelsize=14
URxvt.scrollTtyKeypress: true
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.letterSpace: 0
URxvt.externalBorder: 0

URxvt.iso14755: false
URxvt.iso14755_52: false

! Extensions
! URxvt.perl-ext-common: default,clipboard,keyboard-select,url-select
URxvt.perl-ext-common: selection-to-clipboard
URxvt.urlLauncher: xdg-open
URxvt.urlButton: 1
URxvt.underlineURLs: True
! URxvt.saveLines: 8192

URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.keysym.C-A-V: perl:clipboard:paste_escaped

! Enable the extended coloring options
rofi.color-enabled: true
!                  bg       border   separator
rofi.color-window: #232c31, #232c31, #232c31
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #232c31, #c5c8c6, #232c31, #484d79, #b5d8f6
rofi.color-active: #d79921, #282828, #d79921, #b5d8f6, #282828
rofi.color-urgent: #cc241d, #282828, #cc241d, #fb4934, #282828


! special
*.foreground:   #9ea7a6
*.background:   #232c31
*.cursorColor:  #9ea7a6

! black
*.color0:       #232c31
*.color8:       #3f4944

! red
*.color1:       #2a5491
*.color9:       #2a5491

! green
